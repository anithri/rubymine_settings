<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_SCALE" value="1.25" />
    <option name="LINE_SPACING" value="0.9" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bMaybe\b.*">
      <option name="FOREGROUND" value="958fc0" />
      <option name="BACKGROUND" value="3c3f41" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bBUILD\b">
      <option name="FOREGROUND" value="9922c0" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="ERROR_STRIPE_COLOR" value="ab960c" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
  </component>
</application>