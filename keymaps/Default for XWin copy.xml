<keymap version="1" name="Default for XWin copy" parent="Default for XWin">
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl subtract" />
  </action>
  <action id="MoveEditorToOppositeTabGroup">
    <keyboard-shortcut first-keystroke="ctrl alt back_space" />
  </action>
  <action id="MoveLineDown" />
  <action id="MoveLineUp" />
  <action id="MoveStatementDown" />
  <action id="MoveStatementUp" />
  <action id="MoveTabDown">
    <keyboard-shortcut first-keystroke="alt open_bracket" />
  </action>
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="alt close_bracket" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt w" second-keystroke="right" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt w" second-keystroke="left" />
  </action>
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="shift ctrl minus" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="shift ctrl back_slash" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
</keymap>