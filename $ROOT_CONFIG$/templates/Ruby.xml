<templateSet group="Ruby">
  <template name="iix" value="it { is_expected.to $END$ }" description="it{ is_expected.to }" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="RUBY" value="true" />
    </context>
  </template>
  <template name="iex" value="it { expect(subject.$SUBJECT$).to $END$ }" description="it{ expect().to $END$ }" toReformat="false" toShortenFQNames="true">
    <variable name="SUBJECT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RUBY" value="true" />
      <option name="RHTML" value="false" />
    </context>
  </template>
  <template name="ies" value="it { expect($SUBJECT$).to $END$ }" description="it{ expect().to }" toReformat="false" toShortenFQNames="true">
    <variable name="SUBJECT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RUBY" value="true" />
      <option name="RHTML" value="false" />
    </context>
  </template>
  <template name="mod" value="module $END$&#10;  $SELECTION$&#10;end" description="module surround" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="RUBY" value="true" />
    </context>
  </template>
  <template name="modread" value="module Paradise&#10;  module Readers&#10;    $SELCTION$&#10;  end&#10;end" description="Paradise::Reader" toReformat="false" toShortenFQNames="true">
    <variable name="SELCTION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RUBY" value="true" />
    </context>
  </template>
  <template name="attry" value="# @!attribute [rw] $name$&#10;#   @return [$TYPE$] $END$" description="attribute doc" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RUBY" value="true" />
    </context>
  </template>
  <template name="attr" value="# @!attribute [rw] $NAME$&#10;#   @return [$TYPE$]&#10;attribute :$NAME$, $TYPE$ $END$ " description="attribute - virtus" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RUBY" value="true" />
    </context>
  </template>
</templateSet>